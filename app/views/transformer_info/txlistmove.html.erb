<h1>การย้ายหม้อแปลง</h1>
<p align=center>
<%if @userid.priv6==1%>
  <%= link_to("เพิ่มข้อมูลการย้ายหม้อแปลงไฟฟ้า", "/transformer_info/txaddmove", :class => 'add') %>
<%end%>
</p>
<p><div class='grid_16'>&nbsp;</div></p>
<p>
<div id="transformers" class="grid_14 prefix_1">
  <table id="transformers_table">
    <thead>
      <tr>
      <th>ID</th>
      <th>หม้อแปลงไฟฟ้า</th>
      <th>Equipment No.</th>
      <th>สถานีเก่า</th>
      <th>สถานีใหม่</th>
	  <%if @userid.priv6==1%>
      <th width=160></th>
	  <%end%>
      </tr>
    </thead>
    <tbody>
      <% if !@txtransfers.blank? %>
        <% for txtransfer in @txtransfers %>
	<tr>
            <td><%= txtransfer.id %></td>
            <td><%= link_to(txtransfer.old_txname, "/transformer_info/txshowmove?id=#{txtransfer.id}") %></td>
            <td><%= txtransfer.egatsn %></td>
            <td><%= txtransfer.station %></td>
            <td><%= txtransfer.new_station %> </td>
			<%if @userid.priv6==1%>
            <td><%= link_to("Edit", "/transformer_info/txeditmove?id=#{txtransfer.id}", :class => 'edit') %> &nbsp;&nbsp;&nbsp;&nbsp;
				<%= link_to("Delete", {:controller => "transformer_info", :action => "txdeletemove",:id =>txtransfer.id}, :confirm => 'ยืนยันการลบข้อมูล', :class => 'delete') %></td>
			<%end%>
	</tr>
	<%end%>
      <%end%>
    </tbody>
   </table>
  <%= will_paginate @txtransfers %>
</div>
</p>
