            %td &nbsp;
            %td              
              <img src="http://latex.codecogs.com/gif.latex?\large x_{i}" title="\large x_{i}" />
            %td
              <img src="http://latex.codecogs.com/gif.latex?\large (x_{i} - \bar{x})^2" title="\large (x_{i} - \bar{x})^2" />
            %td 
              <img src="http://latex.codecogs.com/gif.latex?\large x_{i}" title="\large x_{i}" />
              
            %td
